<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dp" android:color="@color/yellow_bd7911" />
            <solid android:color="@color/yellow_bd7911" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="3dip" />
            <stroke android:width="1dp" android:color="@color/yellow_dab270" />
            <solid android:color="@color/yellow_dab270" />
        </shape>
    </item>

</selector>
